pipeline {
  agent any
  stages {
    stage ('Build') {
      steps {
        echo 'Stage Name = Build'
        echo 'Building.....'
        script {
                    def age = 25
                    if(age < 18) {
                        echo "MINOR!!!!!!!!!!"    
                    } else {
                        echo "ADULT!!!!!!!"
                    }
                }
      }
    }
  }
}
