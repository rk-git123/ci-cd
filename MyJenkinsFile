pipeline {
    agent any
    stages {
        stage('Example Build') {
            steps {
                echo 'Hello World'
                script{
                    def p = load "scripts.groovy"
                    def x = p.addNum(10,20)
                    echo x
                }
            }
        }
        stage('Example Deploy') {
            steps {
                echo 'Deploying'
            }
        }
    }
}
